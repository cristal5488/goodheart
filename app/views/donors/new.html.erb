<h1>Become A Donor</h1>

<%= form_for :donor, url: '/donors' do |f| %>

  First_Name: <%= f.text_field :first_name %>
  Last_ Name: <%= f.text_field :last_name %>
  Email: <%= f.text_field :email %>
  Password: <%= f.password_field :password %>
  Password Confirmation: <%= f.password_field :password_confirmation %>
  Zipcode: <%= f.text_field :zipcode %>
  Phone: <%= f.text_field :phone %>
  Bloodtype: <%= f.text_field :bloodtype %>


  <%= f.submit "Submit" %>

<% end %>
